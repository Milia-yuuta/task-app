<div class="col-md-8">
  <% if @users %>
    <ul class="microposts">
      <li>
        name/email/role
      </li>
      <% @users.each do |user| %>
        <li>
          <%= user.email %>
          <%= user.name %>
          <%= user.role %>
        </li>
      <% end %>
    </ul>
    <%= will_paginate @users %>
  <% end %>
</div>